{
    "functions": {
        "api/**/*.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        { "src": "/api/(.*)", "dest": "/api/index.php" },
        { "src": "/api/(.*)", "dest": "/api/functions.php" },
        { "src": "/static/css/(.*)", "dest": "/static/css/$1" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ]
}

